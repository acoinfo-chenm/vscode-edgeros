<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MobileView</title>
    <link rel="stylesheet" href="<%=csspath%>" />
    <script>
        var templates = '<%-JSON.stringify(templates)%>';
        var infoSavePath = "<%-projectDir%>"
    </script>
</head>

<body>
    <div id="app">
        <section id="section">
            <div class="row">
                <div>
                    <div class="type">
                        <span v-for="type in typeArray" :class="selectType==type?'on':''" @click="changeType(type)">
                            {{type}}
                        </span>
                    </div>
                    <div class="single">
                        <span v-for="tpl in templateObjs"
                            :class="'template '+ typeArray.join(' ') + (selectTemp.displayName==tpl.displayName?' on':' ')"
                            @click="changeTpl(tpl)">
                            <div class="img">
                                <img :src="tpl.icon" />
                            </div>
                            <h6>
                                {{tpl.displayName}}
                            </h6>
                        </span>
                    </div>

                </div>
            </div>

            <div class="row">
                <span class="tit">
                    <%=language.discription%>:
                </span>
                <span class="con discription">
                    <p id="discription_txt">{{selectTemp?selectTemp.discription:''}}</p>
                </span>
            </div>

            <div class="row ">
                <span class="tit">
                    <%=language.projectName%>:
                </span>
                <span class="con inputTxt">
                    <input class="projectName" placeholder="<%=language.projectInputName%>" type="text"
                        v-model="projectName" />
                </span>
            </div>

            <div class="row ">
                <span class="tit">
                    <%=language.selectDirectory%>:
                </span>
                <span class="con inputTxt selectDirWarp">
                    <input class="projectDir" v-model="savePath" type="text" />
                    <span class="selectDir" @click="selectDirFn()">
